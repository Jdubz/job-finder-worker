{
  "name": "job-finder-api-server",
  "version": "0.1.0",
  "private": true,
  "description": "Express API replacement for the Job Finder backend",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "migrate": "tsx src/scripts/migrate.ts",
    "migrate:techranks": "tsx src/scripts/migrate-tech-ranks.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.45",
    "@google/gemini-cli": "^0.16.0",
    "@google/generative-ai": "^0.24.1",
    "@openai/codex": "^0.58.0",
    "@shared/types": "file:../../shared",
    "@sparticuz/chromium": "^141.0.0",
    "@types/pdfmake": "^0.2.12",
    "better-sqlite3": "^11.7.0",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "google-auth-library": "^10.5.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "mime-types": "^3.0.1",
    "openai": "^6.3.0",
    "pdfmake": "^0.2.20",
    "pino": "^9.3.0",
    "pino-http": "^10.2.0",
    "puppeteer-core": "^24.24.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.16.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": "^20.0.0"
  }
}
