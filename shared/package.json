{
  "name": "@shared/types",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "dependencies": {
    "zod": "^3.24.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "typecheck": "tsc --noEmit",
    "type-check": "npm run typecheck",
    "dev": "tsup src/index.ts --watch --dts --format esm,cjs --out-dir dist",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.17.50",
    "rimraf": "^5.0.10",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
